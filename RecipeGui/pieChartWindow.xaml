<Window x:Class="RecipeGui.pieChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RecipeGui"
        mc:Ignorable="d"
        Title="pieChartWindow" Height="450" Width="800">
    <Grid Background="#FF88ACDC">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <StackPanel>
            <Button x:Name="PrintChart" Content="Print pie chart" Click="PrintChart"/>
        </StackPanel>
        <CheckBox Content="CheckBox" HorizontalAlignment="Left" Height="780" Margin="1192,334,0,0" VerticalAlignment="Top" Width="416"/>


    </Grid>
</Window>
